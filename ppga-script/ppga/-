-- PPGA STD SYMBOLS
local function __PPGA_INTERNAL_DEFAULT(x, default) 
    if x ~= nil then return (x) end
    return (default)
end
-- END PPGA STD SYMBOLS


local function _HANDLE_ERR(cb, ...)
    local ok, err = ...
    if err ~= nil then
        ok, err = cb(err)
    end
    return (ok), (err)
end

local ok = nil
do
    local _ok_L12S244, _err_L12S244 = __PPGA_INTERNAL_HANDLE_ERR(function (err)
            return (nil), ("WAYTOODANK SOMETHING BROKE")
        end, __PPGA_INTERNAL_HANDLE_ERR(function (err)
            return recovery_request()
        end, some_api_request()))
    if _err_L12S244 ~= nil then
        return (_err_L12S244)
    end
    ok = _ok_L12S244
end